[
  {
    "layer": "top",
    "position": "top",
    "margin-left": 10,
    "margin-right": 10,
    "margin-top": 10,
    "margin-bottom": 0,
    "modules-left": [],
    "modules-center": [
      "hyprland/window"
    ],
    "modules-right": [
      "custom/hyprshot",
      "custom/sunset",
      "bluetooth",
      // "network",
      "pulseaudio",
      "clock",
      "custom/notification"
    ],
    "hyprland/window": {
      "format": "<span  weight='bold' >{class}</span>",
      "max-length": 120,
      "icon": true,
      "icon-size": 13
    },
    // "network": {
    //   "tooltip": true,
    //   "format-wifi": "{icon}",
    //   "format-icons": [
    //     "󰤟",
    //     "󰤢",
    //     "󰤥"
    //   ],
    //   "rotate": 0,
    //   "format-ethernet": "󰈀 ",
    //   "tooltip-format": "Network: <big><b>{essid}</b></big>\nSignal strength: <b>{signaldBm}dBm ({signalStrength}%)</b>\nFrequency: <b>{frequency}MHz</b>\nInterface: <b>{ifname}</b>\nIP: <b>{ipaddr}/{cidr}</b>\nGateway: <b>{gwaddr}</b>\nNetmask: <b>{netmask}</b>",
    //   "format-linked": "󰈀 {ifname} (No IP)",
    //   "format-disconnected": " ",
    //   "tooltip-format-disconnected": "Disconnected",
    //   // "on-click": "/usr/local/bin/ags -t ControlPanel",
    //   "interval": 2,
    // },
    "clock": {
      "format": "{:%I:%M %p}",
      "rotate": 0,
      "on-click": "/usr/local/bin/ags -t ActivityCenter",
      "tooltip-format": "<tt>{calendar}</tt>",
      "calendar": {
        "mode": "month",
        "mode-mon-col": 3,
        "on-scroll": 1,
        "on-click-right": "mode",
        "format": {
          "months": "<span color='#ffead3'><b>{}</b></span>",
          "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
          "today": "<span color='#ff6699'><b>{}</b></span>"
        }
      }
    },
    "custom/notification": {
      "tooltip": false,
      "format": "{icon}",
      "format-icons": {
        "notification": "󰅸",
        "none": "󰂜",
        "dnd-notification": "󰅸",
        "dnd-none": "󱏨",
        "inhibited-notification": "󰅸",
        "inhibited-none": "󰂜",
        "dnd-inhibited-notification": "󰅸",
        "dnd-inhibited-none": "󱏨"
      },
      "return-type": "json",
      "exec-if": "which swaync-client",
      "exec": "swaync-client -swb",
      "on-click-right": "swaync-client -d -sw",
      "on-click": "swaync-client -t -sw",
      "escape": true
    },
    "pulseaudio": {
      "format": "{icon}",
      "rotate": 0,
      "format-muted": "婢",
      "tooltip-format": "{desc} - {volume}%",
      "scroll-step": 5,
      "format-icons": {
        "headphone": "",
        "hands-free": "",
        "headset": "",
        "phone": "",
        "portable": "",
        "car": "",
        "default": [
          "",
          "",
          ""
        ]
      },
      "on-click": "pavucontrol"
    },
    "custom/sunset": {
      "format": "",
      "on-click": "pgrep -x hyprsunset >/dev/null && pkill -x hyprsunset || hyprsunset -t 4000 &"
    },
    "custom/hyprshot": {
      "format": "󰲎",
      "on-click": "hyprshot -m region -o $HOME/Images/Captures"
    },
    "bluetooth": {
      // "format": " {status}",
      "format": "",
      "format-disabled": "",
      "format-connected": "",
      "tooltip-format": "{controller_alias}\t{controller_address}",
      "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{device_enumerate}",
      "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
      "on-click": "blueman-manager"
    }
  }
]